	//	Note : 스킬 레벨 ?E
	//
	++sSkill.wLevel;

	//	Note : 페시?E스킬이 변화 할때 초?E수치들을 모두 ?E계?E磯?
	//
	if ( pSkill->m_sBASIC.emROLE == SKILL::EMROLE_PASSIVE )
	{
		INIT_DATA ( FALSE, FALSE );
	}
}

