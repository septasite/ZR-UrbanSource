						break;
					case EMVAR_CRITICAL_RATE:
						m_sSUMITEM.fIncR_Critical += pItem->sSuitOp.sVARIATE.fVariate;
						break;
					case EMVAR_CRUSHING_BLOW:
						m_sSUMITEM.fIncR_CrushingBlow += pItem->sSuitOp.sVARIATE.fVariate;
